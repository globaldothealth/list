{
    "parser": {
        "awsLambdaArn": "arn:aws:lambda:region:id:function:name"
    },
    "schedule": {
        "awsRuleArn": "arn:aws:events:region:id:rule/name",
        "awsScheduleExpression": "cron(0 10 * * ? *)"
    },
    "dateFilter": {
        "numDaysBeforeToday": 3,
        "op": "EQ"
    }
}