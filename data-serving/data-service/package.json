{
  "name": "data-service",
  "version": "1.0.0",
  "description": "Centralized dataserver for epidemiological line list data",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-covid-data/healthmap-gdo-temp",
    "directory": "data-serving/data-service"
  },
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "import-data": "mongoimport --db covid19 --collection cases --file ../samples/line-list.json",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "node dist/server.js",
    "test": "jest --clearCache && jest --forceExit --coverage --verbose --detectOpenHandles",
    "test-watch": "jest --clearCache && jest --verbose --detectOpenHandles --watch",
    "test-watch-all": "jest --clearCache && jest --coverage --verbose --detectOpenHandles --watchAll",
    "test-silent": "jest --forceExit --silent --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.8.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/mongodb": "^3.5.8",
    "@types/mongoose": "^5.7.12",
    "@types/node": "^13.11.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "mongodb": "^3.5.6",
    "mongoose": "^5.9.10"
  }
}
