    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^27.2.5",
    "lint-staged": "^10.4.0",
    "msw": "^0.28.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
