{
  "name": "data-service",
  "version": "1.0.0",
  "description": "Centralized dataserver for epidemiological line list data",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-covid-data/healthmap-gdo-temp",
    "directory": "data-serving/data-service"
  },
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "nodemon",
    "import-data": "mongoimport --db covid19 --collection cases --jsonArray --file ../samples/cases.json",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "node dist/server.js",
    "test": "jest --clearCache && jest --forceExit --coverage --verbose --detectOpenHandles",
    "test-watch": "jest --clearCache && jest --verbose --detectOpenHandles --watch",
    "test-watch-all": "jest --clearCache && jest --coverage --verbose --detectOpenHandles --watchAll",
    "test-silent": "jest --forceExit --silent --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/mongodb": "^3.5.18",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^13.13.8",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.15"
  }
}