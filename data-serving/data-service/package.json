{
  "name": "data-service",
  "version": "1.0.0",
  "description": "Centralized dataserver for epidemiological line list data",
  "repository": {
    "type": "git",
    "url": "https://github.com/globaldothealth/list",
    "directory": "data-serving/data-service"
  },
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "nodemon",
    "import-data": "mongoimport --db covid19 --collection cases --jsonArray --file ../samples/cases.json",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "node dist/server.js",
    "test": "jest --clearCache && jest --forceExit --coverage --verbose --detectOpenHandles",
    "test-watch": "jest --clearCache && jest --verbose --detectOpenHandles --watch",
    "test-watch-all": "jest --clearCache && jest --coverage --verbose --detectOpenHandles --watchAll",
    "test-silent": "jest --forceExit --silent --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.2",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.12",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.7",
    "@types/express-status-monitor": "^1.2.2",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/mongodb": "^3.5.26",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/multer": "^1.4.4",
    "@types/node-fetch": "^2.5.7",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "body-parser": "^1.19.0",
    "csv-stringify": "^5.5.1",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.10",
    "express-status-monitor": "^1.3.3",
    "express-validator": "^6.6.1",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.0",
    "mongoose": "^5.10.0",
    "node-fetch": "^2.6.1",
    "search-query-parser": "^1.5.5",
    "swagger-ui-express": "^4.1.4",
    "yamljs": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
